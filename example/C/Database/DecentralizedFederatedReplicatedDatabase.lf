target C {
    timeout: 1 sec,
    coordination: decentralized
}

import Platform from "ReplicatedDatabase.lf"

federated reactor ReplicatedDatabase { 
    a = new Platform(query_offset = 10 sec, database_name = "a", STP = 1 msec);
    b = new Platform(query_offset = 10 sec, update_period = 50 msec, start = 100, STP = 50 msec, database_name = "b");
    a.publish -> b.update;
    b.publish -> a.update after 0;
}
