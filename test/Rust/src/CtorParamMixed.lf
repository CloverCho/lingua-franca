target Rust;

reactor Print(
    value: i32(42),
    name: String({="xxx".into()=}),
    other: bool(false)
) {
    reaction(startup) {=
        assert_eq!(42, params.v);
        assert_eq!("x2hr", params.n.as_str());
        assert_eq!(true, params.other);
    =}
}
main reactor CtorParamMixed {
    p = new Print(other=true, name={="x2hr".into()=});
}
