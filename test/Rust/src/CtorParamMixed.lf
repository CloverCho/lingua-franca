target Rust;

reactor Print(
    value: i32(42),
    name: String({="xxx".into()=}),
    other: bool(false)
) {
    state v: i32(value);
    state n: String(name);
    state other: bool(other);

    reaction(startup) {=
        assert_eq!(42, self.v);
        assert_eq!("x2hr", self.n.as_str());
        assert_eq!(true, self.other);
    =}
}
main reactor CtorParamMixed {
    p = new Print(other=true, name={="x2hr".into()=});
}
