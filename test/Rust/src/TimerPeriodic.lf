target Rust {
    timeout: 2 sec,
    // fast: true
};
main reactor TimerPeriodic {
    state i: i32(0);
    timer t2(0, 40 msec);
    reaction(t2) {=
        println!("Tick {} after {} ms", self.i, ctx.get_elapsed_physical_time().as_millis());
        self.i += 1;
    =}

    reaction(shutdown) {=
        assert_eq!(self.i, 51);
        println!("success");
    =}
}
