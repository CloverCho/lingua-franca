/**
 * This test is a variant of HelloWorld.lf that checks the compatibility
 * of the federated C runtime with g++ and no CMake. This test should not pass if it does not compile.
 */
target CCpp {
    tracing: true,
    coordination: centralized,
    timeout: 1 sec,
    cmake: false
};
import HelloWorld from "HelloWorldCCPP.lf";
federated reactor {
    a = new HelloWorld();
    b = new HelloWorld(); // Create a non-singleton federation
}