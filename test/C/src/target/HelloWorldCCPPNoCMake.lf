/**
 * This test is a variant of HelloWorld.lf that checks the basic compatibility
 * of the CCpp target in conjunction with the legacy build system. This test 
 * should not pass if it does not compile.
 */
target CCpp {
    tracing: true,
    logging: DEBUG,
    cmake: false
};
import HelloWorld from "HelloWorldCCPP.lf";
main reactor {
    a = new HelloWorld();
}