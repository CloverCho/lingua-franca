target C {
    tracing: true,
    logging: error,
    docker: true,
    docker-options: {FROM: "alpine:latest"} 
};
reactor HelloWorld {
    state success:bool(false);
    reaction(startup) {=
        printf("Hello World.\n");
        self->success = true;
    =}
    reaction(shutdown) {=
        printf("Shutdown invoked.\n");
        if (!self->success) {
            fprintf(stderr, "ERROR: startup reaction not executed.\n");
            exit(1);
        }
    =}
}
main reactor HelloWorldTest {
    a = new HelloWorld();
}