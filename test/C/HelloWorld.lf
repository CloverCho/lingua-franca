target C {
    tracing: true,
    threads: 4 
};
preamble {=
    #define VERBOSE 
=}
reactor HelloWorld {
    state success:bool(false);
    reaction(startup) {=
        printf("Hello World.\n");
        self->success = true;
    =}
    reaction(shutdown) {=
        printf("Shutdown invoked.\n");
        if (!self->success) {
            fprintf(stderr, "ERROR: startup reaction not executed.\n");
            exit(1);
        }
    =}
}
main reactor HelloWorldTest {
    a = new HelloWorld();
}