/**
 * Test that cmake-include is working correctly.
 * The failure for this test is failure to compile.
 */
target Cpp {
    cmake-include: ["../include/mlib-cmake-extension.cmake", "../include/mlib-cmake-extension.txt"],
    timeout: 0 sec
};


main reactor {
    private preamble {=
        #include <cmath>
    =}
    reaction (startup) {=
        std::cout << "Maximum of 4.20 and 3 is %f" << fmax(4.20, 3.0) << std::endl;
    =}
}
