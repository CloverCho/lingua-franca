/**
 * Test that the link-libraries target property is working correctly.
 * The failure for this test is failure to compile.
 */
target Cpp {
    link-libraries: "m",
    timeout: 0 sec
};

reactor Foo {
    public preamble {=
        #include <cmath>
    =}
    reaction (startup) {=
        std::cout << "Maximum of 42.0 and 75 is " << fmax(4.20, 75) << std::endl;
    =}
}

main reactor {
    f = new Foo();
}
