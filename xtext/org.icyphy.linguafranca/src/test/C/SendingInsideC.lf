target C(
	compile = "cc SendingInsideC.c pqueue.c reactor.c -o SendingInsideC",
	run = "./SendingInsideC -fast -stop 10 sec"
);
reactor Printer {
	input x:int;
	reaction(x) {=
		printf("%d\n", x);
	=}
}
reactor Main {
	state count:int(0);
	timer t(0, 1 sec);
	p = new Printer();
	reaction(t) -> p.x {=
		(self->count)++;
		set(p.x, self->count);
	=}
}