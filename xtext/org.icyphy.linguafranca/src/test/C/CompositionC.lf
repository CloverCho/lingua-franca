target C(
	compile = "cc CompositionC.c pqueue.c reactor.c -o CompositionC",
	run = "./CompositionC -fast -stop 10 sec"
);
reactor Source(period:time(2 sec)) {
	output y:int;
	timer t(1 sec, period);
	state count:int(0);
	reaction(t) -> y {=
		(self->count)++;
		set(y, self->count);
	=}
}
reactor Test {
	input x:int;
	reaction(x) {=
		printf("%d\n", x);
	=}
}
reactor Main {
	s = new Source();
	d = new Test();
	s.y -> d.x;
}