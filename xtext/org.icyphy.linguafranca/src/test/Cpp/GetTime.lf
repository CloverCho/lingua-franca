// This file includes code documented on the Wiki.
// For this test, success is just compiling and running.
target Cpp {
    timeout: 2 sec,
    fast: true
};
main reactor GetTime {
    preamble {=
        #include <iostream>
    =}
    timer t({=reactor::Duration::zero()=}, 1 sec);
    reaction(t) {=
        auto logical = get_logical_time();
        std::cout << "Logical time is " << logical.time_since_epoch().count() << std::endl;

        auto elapsed = get_elapsed_logical_time();
        std::cout << "Elapsed logical time is " << elapsed.count() << std::endl;
 
        auto physical = get_physical_time();
        std::cout << "Physical time is " << physical.time_since_epoch().count() << std::endl;

        auto diff = physical - logical;
        std::cout << "Time lag is " << diff.count() << std::endl;
    =}
}
