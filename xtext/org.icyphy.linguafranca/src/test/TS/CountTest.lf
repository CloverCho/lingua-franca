target TypeScript{
    timeout : 3 sec
};

import Count.lf;
reactor Test {
    input c:number;
    state i:number(0);
    reaction(c) {=
        console.log("Received " + c);
        i++;
        if (c != i) {
            console.log("ERROR: Expected " + i + " but got " + c);
            this.util.exec.failure();
            //throw new Error("ERROR: Expected " + (this.state as any).i + " but got " + (this.state as any).c.get());
        }
    =}
}
main reactor CountTest {
    count = new Count();
    test = new Test();
    count.c -> test.c;
}