target TypeScript;
reactor Printer {
    input x:number;
    reaction(x) {=
        console.log("Inside reactor received:" + (this.state as any).x.get() );
    =}
}
main reactor SendingInside2 {
    timer t;
    p = new Printer();
    reaction(t) -> p.x {=
        (this.state as any).p.x.set(1);
    =}
}
